number = {6},
pages = {2443--2453},
title = {{Simulation of the mushroom cloud generated from a high-energy explosion using large-eddy simulation}},
url = {https://link.springer.com/article/10.1007%2Fs12206-020-0520-x},
volume = {34},
year = {2020}
