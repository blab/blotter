@article{Nadeem2013b,
abstract = {In the present article, we considered two-dimensional steady incompressible Oldroyd-B nanofluid flow past a stretching sheet. Using appropriate similarity variables, the partial differential equations are transformed to ordinary (similarity) equations, which are then solved numerically. The effects of various parameters, namely, Deborah numbers and $\beta$1, Prandtl parameter $\beta$2, Brownian motion Pr, thermophoresis parameter and Lewis number N, on flow and heat transfer are investigated. To see the validity of the present results, we have made the comparison of present results with the existing literature. Copyright: {\textcopyright} 2013 Nadeem et al.},
